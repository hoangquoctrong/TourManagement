<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:TourManagementSystem.EmployeeView.View"
    xmlns:viewmodel="clr-namespace:TourManagementSystem.EmployeeView.ViewModel">

    <DataTemplate DataType="{x:Type viewmodel:EDashboardViewModel}">
        <view:EDashboardView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:ETourViewModel}">
        <view:ETourView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:ETravelGroupViewModel}">
        <view:ETravelGroupView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:EAccountViewModel}">
        <view:EAccountView />
    </DataTemplate>
</ResourceDictionary>